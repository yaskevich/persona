{
  "name": "persona-frontend",
  "version": "0.4.0",
  "description": "Client UI for Persona project",
  "author": "Alyaxey Yaskevich",
  "scripts": {
    "serve": "vite preview",
    "build": "vuedx-typecheck . && vite build",
    "dev": "vite"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "^1.0.2-beta.44",
    "tailwindcss": "^2.1.2",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~v5.0.0-alpha.7",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "eslint-plugin-vue": "^7.9.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "requireConfigFile": false,
        "babelOptions": {
          "plugins": [
            "@babel/plugin-proposal-class-properties"
          ]
        }
      }
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "ISC"
}
